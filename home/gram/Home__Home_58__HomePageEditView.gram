public <Home> = <Home_58__HomePageEditView>;

<Home_58__HomePageEditView> = 
    ( <ThatPage> <Home_Mainpage>[(로|으로)] (<Common_Do>|<Home_Change>|<Common_Save>) ) ;

<ThatPage> = ( <Home_Param_Page_HomePageSetMainPage> [번째|째] | <Home_Param_PageLocPageAnapho_HomePageSetMainPage> ) <Home_Space> (를|을) ;
